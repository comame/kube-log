[Unit]
Description=Kubernetes cluster logging service
Wants=network-online.target
After=network-online.target

[Service]
ExecStart=/usr/local/bin/kube-log/kube-log
Restart=always
Environment=MYSQL_HOST=mysql.comame.dev
Environment=MYSQL_USER=root
Environment=MYSQL_PASSWORD=root
Environment=MYSQL_DATABASE=log

[Install]
WantedBy=multi-user.target
